{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "dotrade Docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "dotrade Docs",
        "groups": [
          {
            "group": "About dotrade",
            "pages": [
              "index",
              "about/core-contributors"
            ]
          },
          {
            "group": "Integrated Protocols",
            "pages": [
              "protocols/hyperliquid",
              "protocols/extended",
              "protocols/lighter",
              "protocols/polymarket"
            ]
          },
          {
            "group": "Products",
            "pages": [
              "products/telegram-bot",
              "products/delta-neutral-vaults"
            ]
          }
        ]
      },
      {
        "tab": "Support",
        "groups": [
          {
            "group": "Resources",
            "pages": [
              "support/contact",
              "support/faq"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://t.me/dotrade",
          "icon": "telegram"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/dotradelabs",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://t.me/dotrade"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://t.me/dotrade_bot"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/dotradelabs",
      "github": "https://github.com/dotradelabs",
      "telegram": "https://t.me/dotrade"
    }
  }
}
