    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://www.dotrade.xyz/"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/dotradelabs",
      "github": "https://github.com/dotradelabs"
    }
  }
}