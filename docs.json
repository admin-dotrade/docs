{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "dotrade",
  "colors": {
    "primary": "#337E81",
    "light": "#24b3bb",
    "dark": "#045559"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guide",
        "groups": [
          {
            "group": "About",
            "pages": [
              "index",
              "about/core-contributors"
            ]
          },
          {
            "group": "Integrated Protocols",
            "pages": [
              "protocols/hyperliquid",
              "protocols/extended",
              "protocols/lighter",
              "protocols/polymarket"
            ]
          },
          {
            "group": "Products",
            "pages": [
              "products/telegram-bot",
              "products/neutral-vaults",
              "products/bridge"
            ]
          }
        ]
      },
      {
        "tab": "Support",
        "groups": [
          {
            "group": "Resources",
            "pages": [
              "support/contact",
              "support/faq"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://x.com/dotradelabs"
      }
